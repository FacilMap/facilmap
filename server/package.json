    "csv-stringify": "^6.5.2",
    "dotenv": "^16.4.7",
    "ejs": "^3.1.10",
    "express": "5.0.1",
    "express-domain-middleware": "^0.1.0",
    "facilmap-frontend": "workspace:^",
    "facilmap-leaflet": "workspace:^",
    "facilmap-types": "workspace:^",
    "facilmap-utils": "workspace:^",
    "finalhandler": "^2.1.0",
    "find-cache-dir": "^5.0.0",
    "i18next": "^24.2.2",
    "i18next-http-middleware": "^3.7.1",
    "json-stream-es": "^1.2.1",
    "jsonwebtoken": "^9.0.2",
    "lodash-es": "^4.17.21",
    "maxmind": "^4.3.24",
    "md5-file": "^5.0.0",
    "mysql2": "^3.12.0",
    "osm-api": "^2.4.0",
    "p-throttle": "^7.0.0",
    "pg": "^8.13.3",
    "proxy-addr": "^2.0.7",
    "sax": "^1.4.1",
    "sequelize": "^6.37.5",
    "sequelize-cursor-pagination": "^3.5.0",
    "serialize-error": "^12.0.0",
    "socket.io": "^4.8.1",
    "string-similarity": "^4.0.4",
    "unbzip2-stream": "^1.4.3",
