{
  "name": "facilmap-server",
  "version": "3.4.0",
  "type": "module",
  "description": "A fully-featured OpenStreetMap-based map where markers and lines can be added with live collaboration.",
  "keywords": [
    "webpack",
    "maps",
    "osm"
  ],
  "homepage": "https://github.com/FacilMap/facilmap",
  "bugs": {
    "url": "https://github.com/FacilMap/facilmap/issues"
  },
  "license": "AGPL-3.0",
  "author": "Candid Dauth <cdauth@cdauth.eu>",
  "repository": {
    "type": "git",
    "url": "https://github.com/FacilMap/facilmap.git"
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "tsconfig.json"
  ],
  "scripts": {
    "start": "npm run deps && npm run server",
    "deps": "npm install",
    "build": "tsc && chmod +x dist/server.js && cpy --cwd=src '**/*' '!**/*.ts' ../dist/",
    "clean": "rimraf dist",
    "server": "dist/server.js dotenv_config_path=../config.env",
    "ts-server": "ts-node --esm --transpile-only src/server.ts dotenv_config_path=../config.env",
    "dev-server": "FM_DEV=true ts-node --esm --transpile-only src/server.ts dotenv_config_path=../config.env",
    "test": "jest",
    "types": "tsc --noEmit src/**/*.ts",
    "lint": "eslint src/**/*.ts"
  },
  "bin": "./dist/server.js",
  "dependencies": {
    "@mapbox/polyline": "^1.1.1",
    "async": "^3.2.4",
    "cheerio": "^1.0.0-rc.12",
    "compression": "^1.7.4",
    "compressjs": "^1.0.3",
    "dotenv": "^16.0.3",
    "ejs": "^3.1.9",
    "event-stream": "^4.0.1",
    "express": "^4.18.2",
    "express-domain-middleware": "^0.1.0",
    "facilmap-frontend": "3.4.0",
    "facilmap-leaflet": "3.4.0",
    "facilmap-types": "3.4.0",
    "facilmap-utils": "3.4.0",
    "highland": "^2.13.5",
    "json-format": "^1.0.1",
    "leaflet": "^1.9.3",
    "lodash-es": "^4.17.21",
    "maxmind": "^4.3.10",
    "md5-file": "^5.0.0",
    "mysql2": "^2.3.3",
    "node-cron": "^3.0.2",
    "node-fetch": "^3.3.1",
    "p-throttle": "^5.0.0",
    "promisify-node": "^0.5.0",
    "sequelize": "6.13",
    "socket.io": "^4.6.1",
    "stream-array": "^1.1.2",
    "stream-combiner": "^0.2.2",
    "string-similarity": "^4.0.4",
    "strip-bom-buf": "^3.0.1",
    "unzipper": "^0.10.11"
  },
  "devDependencies": {
    "@types/async": "^3.2.18",
    "@types/cheerio": "^0.22.31",
    "@types/compression": "^1.7.2",
    "@types/debug": "^4.1.7",
    "@types/ejs": "^3.0.5",
    "@types/event-stream": "^4.0.0",
    "@types/express": "^4.17.17",
    "@types/express-domain-middleware": "^0.0.6",
    "@types/geojson": "^7946.0.10",
    "@types/highland": "^2.12.14",
    "@types/jest": "^29.5.0",
    "@types/lodash-es": "^4.17.7",
    "@types/mapbox__polyline": "^1.0.2",
    "@types/node-cron": "^3.0.7",
    "@types/socket.io": "^3.0.2",
    "@types/string-similarity": "^4.0.0",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "cpy-cli": "^4.2.0",
    "debug": "^4.3.4",
    "jest": "^29.5.0",
    "rimraf": "^4.4.1",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "webpack": "^5.88.2",
    "webpack-dev-middleware": "^6.1.1",
    "webpack-hot-middleware": "^2.25.4"
  }
}
